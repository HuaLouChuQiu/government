page{padding-top: 140rpx; font-family: "Roboto"}
.wxNavgationBar{width: 100%; height: 140rpx; position: fixed; background: white; top: 0; display: flex; flex-direction: column-reverse}
.wxNavgationBar .AppName{display: block; width: 100%; height: 60rpx; padding-bottom: 10rpx; text-align: center; font: 600 28rpx/60rpx 'Roboto'}
.sort-tab{width: 100%; height: 55rpx; background: white; display: flex; flex-direction: column}
.sort-tab .tab-outer{width: 390rpx; height: 55rpx; background: white; padding: 0 10rpx; margin: 0 auto}
.sort-tab .tab-items{width: 100%; height: 50rpx; background: white; -webkit-transition: all 200ms; transition: all 200ms; display: flex; flex-wrap: nowrap; justify-content: center;}
.sort-tab .tab-items text{display: block; font-size: 28rpx; line-height: 50rpx; padding: 0 25rpx; text-align: center; width: 80rpx; float: left; color: #888; letter-spacing: 2px; text-indent: 2px}
.sort-tab .tab-items text:active{background-color: #f0f0f0; -webkit-transition: all 100ms; transition: all 100ms}
.active{color: black}
.sort-tab .indicator-box{width: 100%; height: 5rpx; background: white; display: flex}
.sort-tab .indicator-box .indicator{display: block; width: 35rpx; height: 5rpx; border-radius: 2.5rpx; background: blue; -webkit-transition: all 200ms; transition: all 200ms; margin: 0 auto}
.content, .inner-content{width: 100%; height: 1120rpx; background: #eee}
.inner-content{background: white; height: 1120rpx; }
.test, .text{width: 100%; height: 300rpx; background: yellow}
.text{background: yellowgreen}
.ranking{width: 100%; height: 500rpx; background: none; font-family: "Roboto"}
.ranking-title{width: calc(100%-40rpx); height: 200rpx; background: #eee; display: flex; flex-direction: column-reverse; padding: 0 20rpx}
.ranking-title .title1{width: 100%; height: 40rpx; background: none; font-size: 20rpx; }
.ranking-title .title2{width: 100%; height: 100rpx; background: none; font-size: 34rpx; line-height: 70rpx; font-weight: 900; letter-spacing: 1px; text-indent: 1px}
.ranking-title .title3{width: 100%; height: 60rpx; background: none; font-size: 32rpx; font-weight: 700; display: flex; flex-direction: column-reverse}
.ranking-title .title2 text, .ranking-title .title3 text{border-left: 4px solid #E2100D; display: block; text-indent: 15rpx; color: #E2100D; background: white}
.ranking-title .title1 text{border-left: 4px solid #2933E1; display: block; text-indent: 15rpx}
.medal{width: 80rpx; height: 80rpx; background: green; float: right; position: absolute}
.ranking-content{width: calc(100%-40rpx); height: 300rpx; background: #eee; padding: 0 20rpx; padding-bottom: 5rpx}
.ranking-content::after{content: ""; display: block; width: 100%; height: 20rpx;}
.ranking-top-items{width: inherit; height: 255rpx; background: none}
.ranking-top-items .tops{width: calc(100%-60rpx); height: 85rpx; background: #fff}
.tops .goal-icon{float: left; width: 85rpx; height: 85rpx}
.tops .goal-icon image{width: 55rpx; height: 55rpx; padding: 15rpx}
.tops .goal-name{float: left; width: calc(100%-185rpx); height: 85rpx; display: flex; align-items: center}
.tops .goal-name text{display: block; width: calc(100%-1px); border-left: 1px solid #eee; height: 40rpx; font-size: 24rpx; line-height: 40rpx; text-indent: 10rpx}
.tops .goal-more{width: 100rpx; height: 85rpx; background: none; float: right; display: flex; justify-content: center; align-items: center; padding-right: 20rpx}
.tops:active{background: #ebebeb; }
.view_more{display: block; height: 40rpx; width: 85rpx; border-radius: 20rpx; border: 1px solid #4082fc; font-size: 24rpx; color: #4082fc; text-align: center; line-height: 40rpx}
.view_more:active{background-color: #4082fc; color: #fff; -webkit-transition: all 100ms; transition: all 100ms}
.ranking-next-items{width: 100%; height: 60rpx; }
.ranking-next-items .rank-order{width: 45rpx; height: 60rpx; float: left; background: white; padding: 0 20rpx; display: flex; align-items: center; justify-content: center; border-right: 1px solid #eee}
.ranking-next-items .rank-order text{display: block; font-size: 28rpx; line-height: 60rpx; text-align: center; font-weight: 600; color: #ffca28}
.ranking-next-items .rank-name{float: left; width: calc(100%-165rpx); height: 60rpx; background: white; text-indent: 10rpx; font-size: 24rpx; line-height: 60rpx;}
.view_all{width: 140rpx; background: none; height: 60rpx; font-size: 30rpx; line-height: 60rpx; color: rgba(0, 0, 0, .5); text-align: center; position: relative; z-index: 2; font-weight: bolder; letter-spacing: 1px; text-indent: 1px; margin: 0 auto}
.view_all:active{-webkit-transition: all 100ms; transition: all 100ms; color: #4082fc}
#redirection{margin-top: -60rpx; position: relative; z-index: inherit; background-image: linear-gradient(0, #ddd 0%, rgba(255, 255, 255, 0) 100%); padding: 5rpx 0}
#redirection::after{display: block; content: ""; width: 100%; height: 2px; -webkit-transform: skewY(30deg) rotate(-30deg) scaleX(1.5); transform: skewY(30deg) rotate(-30deg) scale(1.5); background-image: linear-gradient( to right, #2933E1, #2933E1 12.5%, #E2100D 12.5%, #E2100D 25%, #2933E1 25%, #2933E1 37.5%, #E2100D 37.5%, #E2100D 50%, #2933E1 50%, #2933E1 62.5%, #E2100D 62.5%, #E2100D 75%, #2933E1 75%, #2933E1 87.5%, #E2100D 87.5%, #E2100D);}
.order-rank{width: 55rpx; height: 55rpx; position: absolute; padding: 15rpx; text-align: center; font-size: 20rpx; font-weight: 700; line-height: 40rpx; color: rgba(255, 255, 255, .5)}
.goaler{color: #282828; font-weight: bolder}

.passages-country{width: calc(100%-20rpx); padding: 35rpx 0 0}
.passages-country .partPorts:last-child{border: 0}
.partName{width: inherit; height: 30rpx; background: none; font: bolder 25rpx/30rpx "Roboto"; text-indent: 10rpx; border-left: 4px solid #4082fc; margin: 0 10rpx 20rpx}
.partPorts{width: calc(100%-40rpx); height: 240rpx; background: none; padding: 0 20rpx; padding-bottom: 22rpx; border-bottom: 1px dashed rgba(0, 0, 0, .08)}
.part-header{width: 100%; height: 220rpx; background: none}
.part-footer{width: 100%; height: 20rpx; background: none; font-size: 18rpx; line-height: 20rpx; color: #989898;}
.part-left{width: 700rpx; float: left; height: 210rpx;  background: white; padding: 10rpx 5rpx 0 0}
.part-right{width: 0rpx; height: 100%; background: black; float: right;}
.part-title{width: inherit; height: 80rpx; background: none; font: 900 26rpx/40rpx "Roboto"; padding-bottom: 20rpx}
.part-content{width: inherit; height: 90rpx; color: #989898; font: 400 20rpx/30rpx "Roboto"; padding-bottom: 20rpx}
.time-stamp{display: block; float: left;}
.origin-site{display: block; float: right}
.mark-stamp{width: 20rpx; height: 20rpx; border-radius: 50%; background: #c00; margin: 0 10rpx; display: block; float: right}
.r_selector{width: 100%; height: 60rpx; background: none; font: 400 26rpx/60rpx "Roboto"}
.currProvince{width: 150rpx; height: 60rpx; padding-left: 25rpx; border-radius: 30rpx; border: 1px solid rgba(0, 0, 0, .08); float: left; background: none; margin-left: 20rpx; color: rgba(0, 0, 0, .4)}
.currProvince:active, .reGPS:active{-webkit-transition: all 100ms; transition: all 100ms; background: rgba(0, 0, 0, .1); color: white}
.currProvince scroll-view{width: 90rpx; float: left; font: 400 26rpx/60rpx "Roboto"; }
.currProvince scroll-view text{white-space: nowrap}
.currProvince image{width: 60rpx; float: left; display: block; width: 60rpx; height: 60rpx; -webkit-transform: rotate(90deg); transform: rotate(90deg)}
.reGPS{width: 185rpx; height: 60rpx; float: right; margin-right: 20rpx; border-radius: 5px}
.reGPS view{width: 125rpx; height: 60rpx; float: right; }
.reGPS image{width: 50rpx; height: 50rpx; padding: 5rpx}